{
  "manifest_version": 2,

  "name": "LichessTV Embedder",
  "description": "Embeds LichessTV onto websites of your choice",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<http://*.nicovideo.jp/*>", "<http://www.twitch.tv/*>"],
      "js": ["jquery.js", "draggable.js", "app.js"],
      "css": ["style.css"]
    }
  ],
  "web_accessible_resources": [
    "index.html"
  ]
}
